{
  "name": "api",
  "version": "1.0.0",
  "type": "module",
  "engines": { "node": ">=20" },
  "main": "_dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "mkdir -p public && rm -rf _dist && tsc --outDir ./_dist",
    "check:no-nullish": "node scripts/check-no-nullish.cjs",
    "start": "node _dist/index.js",
    "db:seed": "tsx prisma/seed.ts",
    "postinstall": "node scripts/patch-fastify-nullish.cjs"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "@fastify/cors": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "prisma": "^5.22.0",
    "tsx": "^4.19.0",
    "typescript": "^5.5.0"
  }
}
